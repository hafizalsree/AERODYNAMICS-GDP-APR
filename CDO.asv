function CD0 = CDO(condition.cf,component_drag,Swet)

%% Nacelle
cd_nacelle = cf(nacelle.l_nacelle,M,flow_state) * component_drag(1) * Swet(1);


%% Fuselage
cd_fuselage = cf(fuselage.l_fuselage,M,flow_state) * component_drag(2) * Swet(2);


%% Wing
cd_wing = cf(wing.meanChord,M,flow_state) * component_drag(3) * Swet(3);


%% Tail
cd_tail = cf(length,M,flow_state) * component_drag(4) * Swet(4);


%% Total

CD0 = (cd_nacelle + cd_fuselage + cd_wing + cd_tail)/Sref;

end